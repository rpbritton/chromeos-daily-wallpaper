{
	"manifest_version": 2,
	"name": "Daily Wallpaper from Bing (for ChromeOS)",
	"description": "Sets the wallpaper to Bing's image of the day in ChromeOS.",
	"homepage_url": "https://www.google.com",
	"version": "1.0",
	"permissions": [
		"wallpaper",
		"alarms",
		"contextMenus",
		"storage",
		"*://*.bing.com/*"
	],
	"icons": {
		"512": "icon.png"
	},
	"background": {
		"persistant": false,
		"scripts": ["background.js"]
	}
}
